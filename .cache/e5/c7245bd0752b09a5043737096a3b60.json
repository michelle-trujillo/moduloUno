 row;\r\n    justify-content: center;\r\n    font-family: Arial, Helvetica, sans-serif;\r\n\r\n    .box {\r\n      height: 5rem;\r\n      padding: 0.5rem;\r\n      display: flex;\r\n      align-items: center;\r\n      border: 1px solid black;\r\n      border-radius: 0.5rem;\r\n      margin: 0.2rem;\r\n      color: $span-color;\r\n\r\n      &.base-color {\r\n        @include get-color-base-from-list($c);\r\n      }\r\n\r\n      @for $i from 1 to 5 {\r\n        &.lighten-#{$i} {\r\n          background-color: lighten($base-color, $i * $percent-lighten);\r\n        }\r\n      }\r\n\r\n      @for $i from 1 to 5 {\r\n        &.darken-#{$i} {\r\n          background-color: darken($base-color, $i * $percent-darken);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"4fe39c0c6634b50661d56a633ec116bb","cacheData":{"env":{}}}